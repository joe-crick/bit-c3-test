{
  "name": "bit-c3-test",
  "version": "1.0.0",
  "description": "bit-c3 test",
  "main": "app.js",
  "scripts": {
    "build": "steal-tools",
    "debug": "steal-tools --watch",
    "start": "http-server"
  },
  "system": {
    "npmAlgorithm": "flat",
    "npmIgnore": [
      "browser-sync",
      "del",
      "documentjs",
      "glob",
      "gulp",
      "gulp-autoprefixer",
      "gulp-less",
      "gulp-minify-css",
      "gulp-notify",
      "gulp-rename",
      "gulp-replace",
      "gulp-sourcemaps",
      "gulp-util",
      "gulp-jshint",
      "through2",
      "merge2",
      "pretty-hrtime",
      "require-dir",
      "run-sequence",
      "stream-from-promise",
      "testee"
